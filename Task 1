list1 = [12 , -7 , 5 , 64 ,-14]
num = 0

while(num < len(list1)):
    if list1[num] >= 0:
        print(list1[num], end = " ")

    num += 1

print("\n\n\n")


list1 = [12 , 14 , -95 , 3]
num = 0

while(num < len(list1)):
     if list1 [num] >= 0:
        print(list1[num], end = " ")

      
     num += 1




